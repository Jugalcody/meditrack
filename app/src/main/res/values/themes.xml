<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Trackhealth" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">#2C635C</item>
        <item name="colorPrimaryVariant">@color/register_bg_color1</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:statusBarColor">#BEC9D6</item>
        <item name="android:color">#882525</item>

    </style>
    <style name="myfun">
        <item name="android:textSize">34sp</item>
        <item name="android:colorPrimary">#6D78CC</item>
    </style>
    <style name="Theme.Trackhealth" parent="Base.Theme.Trackhealth" >
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="Theme.SplashScreen" parent="Base.Theme.Trackhealth" >
        <item name="colorPrimary">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>


    <style name="Theme.Register_process" parent="Base.Theme.Trackhealth" >
        <item name="colorPrimary">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="Theme.StartFragment" parent="Base.Theme.Trackhealth" >
        <item name="colorPrimary">#C3CAF3</item>
    </style>


    <style name="Theme.Loginpage" parent="Base.Theme.Trackhealth" >
        <item name="colorPrimary">#58ACA1</item>
    </style>

    <style name="Theme.HomePage" parent="Base.Theme.Trackhealth" >
        <item name="colorPrimary">#D37070</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="Theme.RegisterPage" parent="Base.Theme.Trackhealth" >
        <item name="colorPrimary">#FFFFFF</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="ThemeOverlay.AppCompat.navTheme">
        <item name="colorPrimary">@color/white</item>
    </style>

    <style name="App.Custom.Indicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">

        <item name="android:color">#323232</item>
    </style>
</resources>