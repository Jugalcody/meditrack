<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="roundedImageview">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
       
    </style>
    <style name="specificcorner">
        <item name="cornerSizeTopRight">75dp</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeBottomLeft">75dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
    </style>

    <style name="CustomTabLayout" parent="Widget.Design.TabLayout">
        <item name="tabPaddingStart">0dp</item>
        <item name="tabPaddingEnd">1dp</item>
        <item name="tabPaddingTop">1dp</item>
        <item name="tabPaddingBottom">0dp</item>
    </style>


    <style name="CustomTabText" parent="Widget.Design.TabLayout">
        <item name="android:textSize">100sp</item>
    </style>

    <style name="text_active">
        <item name="android:textSize">16sp</item>
    </style>
    <style name="text_inactive">
        <item name="android:textSize">14sp</item>

    </style>

    <style name="createcircle">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">rounded</item>

    </style>


    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="android:progressBackgroundTint">#CFCDCD</item>
        <item name="colorControlActivated">@color/ratingcolor</item>
    </style>
<style name="autotextview" parent="ThemeOverlay.Material3.AutoCompleteTextView">
<item name="android:layout_margin">0dp</item>
    <item name="android:padding">0dp</item>
</style>
</resources>