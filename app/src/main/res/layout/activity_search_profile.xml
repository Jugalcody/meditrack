                    android:layout_marginEnd="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:text="All"
                    android:padding="10dp"
                    android:id="@+id/sdipall"
                    android:button="@color/black"
                    android:textSize="15sp"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="5 star"
                    android:layout_marginEnd="2.5dp"
                    android:layout_marginStart="2.5dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:padding="10dp"
                    android:elevation="4dp"
                    android:button="@color/black"
                    android:id="@+id/sdip5"
                    android:background="@drawable/register_selector_radiobutton"
                    android:textColor="@drawable/register_text_color"
                    android:textSize="@dimen/register_font_size"
                    />


                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4 star"
                    android:layout_marginEnd="2.5dp"
                    android:layout_marginStart="2.5dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:padding="10dp"
                    android:elevation="4dp"
                    android:button="@color/black"
                    android:id="@+id/sdip4"
                    android:background="@drawable/register_selector_radiobutton"
                    android:textColor="@drawable/register_text_color"
                    android:textSize="@dimen/register_font_size"
                    />


                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="3 star"
                    android:layout_marginEnd="2.5dp"
                    android:layout_marginStart="2.5dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:padding="10dp"
                    android:elevation="4dp"
                    android:button="@color/black"
                    android:id="@+id/sdip3"
                    android:background="@drawable/register_selector_radiobutton"
                    android:textColor="@drawable/register_text_color"
                    android:textSize="@dimen/register_font_size"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2 star"
                    android:layout_marginEnd="2.5dp"
                    android:layout_marginStart="2.5dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:padding="10dp"
                    android:elevation="4dp"
                    android:button="@color/black"
                    android:id="@+id/sdip2"
                    android:background="@drawable/register_selector_radiobutton"
                    android:textColor="@drawable/register_text_color"
                    android:textSize="@dimen/register_font_size"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1 star"
                    android:layout_marginEnd="2.5dp"
                    android:layout_marginStart="2.5dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:padding="10dp"
                    android:elevation="4dp"
                    android:button="@color/black"
                    android:id="@+id/sdip1"
                    android:background="@drawable/register_selector_radiobutton"
                    android:textColor="@drawable/register_text_color"
                    android:textSize="@dimen/register_font_size"
                    />
            </RadioGroup>
</HorizontalScrollView>
            <androidx.recyclerview.widget.RecyclerView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_below="@id/profilesearch_hs"
                android:id="@+id/sdiprecreview"
                />
    </RelativeLayout>
</androidx.cardview.widget.CardView>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>