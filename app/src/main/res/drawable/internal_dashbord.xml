<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#EAF6F6" />
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="360">
            <shape android:shape="rectangle">
                <solid android:color="#96D3CB" />
            </shape>
        </rotate>
    </item> <item>
        <rotate android:fromDegrees="90">
            <shape android:shape="rectangle">
                <solid android:color="#79A8DD" />
            </shape>
        </rotate>
    </item>

    <item>
        <rotate android:fromDegrees="190">
            <shape android:shape="rectangle">
                <solid android:color="#E2F1F1" />
                <gradient android:startColor="#8DABCD"
                    android:endColor="#49E6D3"/>
            </shape>

        </rotate>
    </item>
</layer-list>